class Solution:
    def applyOperations(self, nums: List[int]) -> List[int]:
        for i in range(len(nums)-1):
            if nums[i] == nums[i + 1]:
                nums[i]*=2
                nums[i+1]=0        
        later_add = []
        for num in range(len(nums)-1):
            if num == 0:
                later_add.append(num)
                nums.remove(num)
        return nums + later_add
